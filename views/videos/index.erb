<h1><%= @page_header %></h1>

<% @videos.each do |video| %>

	<h2>Video: <%= video.title %></h2>
	<p><iframe src="<%=video.url %>"></iframe></p>
	<p><%= video.description %></p>
	<p><%= video.genre %></p>
	<a href="videos/<%= video.id %>">View</a>
	<a href="videos/<%= video.id %>/edit">Edit</a>

<% end %>